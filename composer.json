{
    "name": "webhooker/webhooker-sdk",
    "type": "library",
    "description": "SDK for communicating with the WebHooker.io API",
    "keywords": [
        "webhooker",
        "webhooker-sdk",
        "webhooks"
    ],
    "homepage": "https://github.com/webhooker/webhooker-sdk-php",
    "license": "MIT",
    "authors": [
        {
            "name": "Dan Harper",
            "email": "dan@radweb.co.uk",
            "homepage": "https://radweb.co.uk",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : ">=5.4.0"
    },
    "require-dev": {
        "phpunit/phpunit" : "4.*",
        "scrutinizer/ocular": "~1.1",
        "squizlabs/php_codesniffer": "~2.3"
    },
    "autoload": {
        "psr-4": {
            "WebHooker\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "WebHooker\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
